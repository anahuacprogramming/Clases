String amigo = "Pedro";

String amigos= amiga+" es amiga de "+amigo;


float incremento=200;


void setup() {
  size(600, 400);
  background(255);
}

void draw() {

  incremento -= 1;

  fill(0, 50);
  rect(mouseX, mouseY, incremento, incremento);
  print(incremento);
}
