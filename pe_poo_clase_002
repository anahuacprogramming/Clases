//Processing suele necesitar dos funciones reservadas
//La primera configura el lienzo: setup()
//La segunda controla el lienzo: draw()

void setup() {
  size(800, 200);
  background(255);
}

void draw() {
  //background(255);
  noStroke();
  fill(random(255), 0, 0, 80);
  ellipse(mouseX, mouseY, 50, 50);
}



