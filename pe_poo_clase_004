float inc =TWO_PI/50;
float a= 0.0;

size(600,400);
background(255);

for (int i = 0; i < 204; i += 4) { 
  stroke(0);
  line(i, 100, i, 100 + sin(a) * 80.0); 
  a = a + inc;
  
}
